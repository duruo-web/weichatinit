<template name="stars_template">
  <view class="star-container">
    <view class="star-images">
      <block wx:for="{{stars}}" wx:for-item="item">
        <image wx:if="{{item}}" src="/images/icon/have-star.png"></image>
        <image wx:else src="/images/icon/none-star.png"></image>
      </block>
    </view>
    <text class="">{{scor}}</text>
  </view>
</template>